{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "connector",
  "version": "2.0.0",
  "identifier": "app.vercel.connector-ten",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:2255",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "connector",
        "width": 1280,
        "height": 820,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDEwQjYwMUIyQkQzOEQyRTEKUldUaDBqaTlzZ0cyRU1wVG13TzJGSmFlS0J3YWxMSWhIM1lMNlJmTStNZGgvN0p1c28yV0FELzEK",
      "endpoints": [
        "https://github.com/user/repo/releases/latest/download/latest.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
